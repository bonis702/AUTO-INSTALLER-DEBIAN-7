	echo "   6) Melihat Daftar akun Dan Expired nya"
	echo "   7) Membuat Akun SSH baru"
	echo "   8) Melihat Pemakaian Bandwith"
	echo "   9) Melihat Port Aktif"
	echo "  10) Restart Semua Service"
	echo "==================== TERIMA KASIH ========================="
	while [[ $MENU != "1" && $MENU != "2" && $MENU != "3" && $MENU != "4" && $MENU != "5" && $MENU != "6" && $MENU != "7" && $MENU != "8" && $MENU != "9" && $MENU != "10" ]]; do
		read -p "Silahkan Pilih Nomornya BOSQU: " -e -i 1 MENU
	done
case $MENU in
		1)
		cd
		login
		;;
		2)
		cd
		memori
		;;
		3)
		cd
		bench
